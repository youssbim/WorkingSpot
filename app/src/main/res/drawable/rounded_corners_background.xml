<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/md_theme_surfaceContainerHigh"/>
            <padding
                android:bottom="@dimen/rounded_background_padding"
                android:left="@dimen/rounded_background_padding"
                android:right="@dimen/rounded_background_padding"
                android:top="@dimen/rounded_background_padding" />
            <corners android:bottomRightRadius="12dp"
                android:bottomLeftRadius="12dp"
                android:topLeftRadius="12dp"
                android:topRightRadius="12dp"/>
        </shape>
    </item>
</layer-list>